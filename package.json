{
  "name": "6starlong.github.io",
  "version": "1.0.1",
  "main": "index.js",
  "repository": "https://github.com/6starlong/6starlong.github.io.git",
  "author": "Starlong <starlong.lu@gmail.com>",
  "license": "MIT",
  "homepage": "https://6starlong.github.io",
  "devDependencies": {
    "chalk": "^4.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "execa": "^5.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "semver": "^7.3.5",
    "vitepress": "^0.22.3",
    "yorkie": "^2.0.0"
  },
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "lint": "eslint --ext .js,.vue docs",
    "format": "prettier --write docs",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,json,vue,md}": [
      "prettier --write"
    ],
    "*.{js,vue}": [
      "eslint"
    ]
  }
}
