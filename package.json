{
  "name": "6starlong.github.io",
  "version": "1.0.3",
  "repository": "https://github.com/6Starlong/6Starlong.github.io",
  "author": "Starlong <starlong.lu@gmail.com>",
  "license": "MIT",
  "homepage": "https://6Starlong.github.io",
  "devDependencies": {
    "@docsearch/css": "^3.0.0",
    "@vue/theme": "^1.0.1",
    "@vueuse/core": "^8.2.5",
    "autoprefixer": "^10.4.4",
    "body-scroll-lock": "^4.0.0-beta.0",
    "chalk": "^4.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "enquirer": "^2.3.6",
    "eslint": "^8.12.0",
    "eslint-plugin-vue": "^8.5.0",
    "execa": "^5.0.0",
    "lint-staged": "^12.3.7",
    "normalize.css": "^8.0.1",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "semver": "^7.3.5",
    "tailwindcss": "^3.0.23",
    "vitepress": "^0.22.3",
    "vue": "^3.2.32",
    "yorkie": "^2.0.0"
  },
  "scripts": {
    "dev": "vitepress",
    "build": "vitepress build",
    "serve": "vitepress serve",
    "lint": "eslint --ext .js,.vue .vitepress src",
    "format": "prettier --write .vitepress src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.js",
    "deploy": "scripts/deploy.sh"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.{js,json,vue,md}": [
      "prettier --write"
    ],
    "*.{js,vue}": [
      "eslint"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  }
}
